 <pump_commands>
	<!-- Black-black tubing rates: 
		48 rpm = 2.14 mL/min 
		30 rpm = 1.34 mL/min
		implies
		0.0446 mL/min/rpm
	-->
	<pump_cmd name = "2 mL/min">
       <pump_config speed = "44.84" direction = "Forward"></pump_config>
    </pump_cmd>
	<pump_cmd name = "1 mL/min">
       <pump_config speed = "22.42" direction = "Forward"></pump_config>
	</pump_cmd>
	<pump_cmd name = "0.5 mL/min">
       <pump_config speed = "11.21" direction = "Forward"></pump_config>
    </pump_cmd>
    <pump_cmd name = "0.25 mL/min">
       <pump_config speed = "5.62" direction = "Forward"></pump_config>
    </pump_cmd>
    <pump_cmd name = "0.1 mL/min">
       <pump_config speed = "2.24" direction = "Forward"></pump_config>
    </pump_cmd>
    <pump_cmd name = "0.01 mL/min">
       <pump_config speed = "0.22" direction = "Forward"></pump_config>
    </pump_cmd>
    <pump_cmd name = "Stop Flow">
       <pump_config speed = "0.0"></pump_config>
    </pump_cmd>
 </pump_commands>

