<?xml version="1.0" encoding="ISO-8859-1"?>
<settings><!-- Default kilroy settings for jfocal -->
  <!-- Valve parameters -->
  <valves_com_port type="string">COM11</valves_com_port><!-- COM port of serial connection to valves -->  
  <num_simulated_valves type="int">0</num_simulated_valves><!-- Number of valves to simulate (Defaults to 0) -->
  <valve_class type="string">valves.hamilton_mvp4</valve_class><!-- Type of valves: Simulated, Hamilton, or Titan -->
	
  <!-- Syringe Pump parameters -->
  <pump_type type="string">syringe</pump_type>
  <pump_class type="string">pumps.hamilton_psd4</pump_class><!-- Control class for pump -->
  <syringe_type type="string">smooth_flow</syringe_type><!-- Set the specific variant of the PSD4 -->
  <pump_com_port type="string">COM12</pump_com_port><!-- COM port of serial connection to pump -->
  <simulate_pump type="boolean">False</simulate_pump><!-- Simulate pump? (Defaults to False) -->
  <flip_flow_direction type="boolean">False</flip_flow_direction><!-- Flip the direction defined as forward? -->
  <port_names type="string">Exhaust,Blocked 1,Pull</port_names>
  <syringe_volume type="float">5.0</syringe_volume>
  <min_speed type="float">0.0125</min_speed><!-- speed limit for pump control text boxes-->
  <max_speed type="float">62.5</max_speed><!-- speed limit for pump control text boxes-->
  <min_volume type="float">0</min_volume><!-- pull limit for pump control text boxes-->
  <max_volume type="float">5.0</max_volume><!-- speed limit for pump control text boxes-->

  <!-- General Kilroy parameters -->
  <verbose type="boolean">True</verbose>
  <serial_verbose type="boolean">True</serial_verbose>  <!-- display serial commands? -->
  <tcp_port type="int">9500</tcp_port> <!-- TCP/IP port for local communication with Dave -->
  <protocols_file type = "">kilroy_config_merfish6_syringe.xml</protocols_file><!-- Location of default protocol -->
  <commands_file type = "">kilroy_config_merfish6_syringe.xml</commands_file><!-- Location of default commands -->

</settings>
